{
    "name": "oro/phpstan-rules",
    "description": "A set of additional PHPStan rules used in Oro products.",
    "license": "MIT",
    "authors": [
        {
            "name": "Oro, Inc",
            "homepage": "https://oroinc.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "phpstan": {
            "includes": [
                "phpstan.neon",
                "extension.neon",
                "rules.neon"
            ]
        }
    },
    "autoload": {
        "psr-4": {"": "src/"}
    },
    "require": {
        "phpstan/phpstan": "1.11.*",
        "phpstan/phpstan-doctrine": "1.4.*",
        "nette/neon": "^3.1"
    }
}
